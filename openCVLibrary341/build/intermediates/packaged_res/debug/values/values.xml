<?xml version="1.0" encoding="utf-8"?>
<resources>
    <color name="blue">#ff59a9ff</color>
    <color name="bottom_background_color">#ffc4c2bc</color>
    <color name="colorAccent">#FF4081</color>
    <color name="colorGrey">#cccccc</color>
    <color name="colorPrimary">#3F51B5</color>
    <color name="colorPrimaryDark">#303F9F</color>
    <color name="orange">#ffff900d</color>
    <color name="polygonViewCircleBackground">#90e5f8ff</color>
    <color name="polygonViewCircleStrokeColor">#ff59a9ff</color>
    <dimen name="activity_horizontal_margin">16dp</dimen>
    <dimen name="activity_vertical_margin">16dp</dimen>
    <dimen name="bottom_bar_padding">15dp</dimen>
    <dimen name="polygonViewCircleWidth">32dp</dimen>
    <dimen name="polygonViewStrokeWidth">2dp</dimen>
    <dimen name="scanPadding">16dp</dimen>
    <string name="app_name">CameraScanner</string>
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/colorPrimary</item>
        <item name="colorPrimaryDark">@color/colorPrimaryDark</item>
        <item name="colorAccent">@color/colorAccent</item>
    </style>
    <declare-styleable name="CameraBridgeViewBase">
       <attr format="boolean" name="show_fps"/>
       <attr format="integer" name="camera_id">
          <enum name="any" value="-1"/>
          <enum name="back" value="99"/>
          <enum name="front" value="98"/>
       </attr>
    </declare-styleable>
</resources>